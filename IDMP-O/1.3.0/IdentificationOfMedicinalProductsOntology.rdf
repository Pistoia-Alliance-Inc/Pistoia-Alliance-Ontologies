			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&idmp-sub;isDerivedFromOrganismFraction"/>
				<owl:allValuesFrom rdf:resource="&idmp-sub;OrganismFraction"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&idmp-sub;isDerivedFromOrganismIdentifiedBy"/>
				<owl:allValuesFrom rdf:resource="&idmp-sub;OrganismIdentifier"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&idmp-sub;isDerivedFromOrganismNamed"/>
				<owl:allValuesFrom rdf:resource="&idmp-sub;OrganismName"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&idmp-sub;isDerivedFromOrganismPart"/>
				<owl:allValuesFrom rdf:resource="&idmp-sub;OrganismPart"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&idmp-sub;hasCountryOfOrigin"/>
