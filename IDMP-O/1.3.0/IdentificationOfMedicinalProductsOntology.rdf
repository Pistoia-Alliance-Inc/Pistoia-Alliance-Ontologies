		<skos:definition>code for an integer datatype</skos:definition>
		<cmns-av:explanatoryNote>This code denotes an xsd:integer value.</cmns-av:explanatoryNote>
		<cmns-col:isMemberOf rdf:resource="&idmp-dtp;ISO21090-CodeSet"/>
		<cmns-dsg:hasDescription>integer</cmns-dsg:hasDescription>
		<cmns-txt:hasTextValue>INT</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-dtp;DatatypeCode-NM">
