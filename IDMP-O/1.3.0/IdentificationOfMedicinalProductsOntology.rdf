		<cmns-col:isMemberOf rdf:resource="&idmp-sub;ISO11238-ResidueModifiedVocabulary"/>
		<cmns-dsg:isDefinedIn rdf:resource="&idmp-sub;ISO11238-ResidueModifiedVocabulary"/>
