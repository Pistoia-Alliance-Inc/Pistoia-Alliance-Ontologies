		<rdfs:label>datatype code ST</rdfs:label>
		<skos:definition>code for a string-valued datatype</skos:definition>
		<cmns-av:explanatoryNote>This code denotes either an xsd:string or rdf:langString.</cmns-av:explanatoryNote>
		<cmns-col:isMemberOf rdf:resource="&idmp-dtp;ISO21090-CodeSet"/>
		<cmns-dsg:hasDescription>string</cmns-dsg:hasDescription>
		<cmns-txt:hasTextValue>ST</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-dtp;DatatypeCode-STT">
		<rdf:type rdf:resource="&cmns-cds;CodeElement"/>
		<rdfs:label>datatype code STT</rdfs:label>
		<skos:definition>code for a datatype representing the condition of something as of some point in time</skos:definition>
		<cmns-col:isMemberOf rdf:resource="&idmp-dtp;ISO21090-CodeSet"/>
		<cmns-dsg:hasDescription>state</cmns-dsg:hasDescription>
		<cmns-txt:hasTextValue>STT</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
