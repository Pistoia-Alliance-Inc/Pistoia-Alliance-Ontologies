		<skos:example>Typical name parts for person names are given names, and family names, titles, etc.</skos:example>
		<cmns-col:isMemberOf rdf:resource="&idmp-dtp;ISO21090-CodeSet"/>
		<cmns-txt:hasTextValue>ENXP</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-dtp;DatatypeCode-HXIT">
		<rdf:type rdf:resource="&cmns-cds;CodeElement"/>
		<rdfs:label>datatype code HXIT</rdfs:label>
		<skos:definition>code representing the top-level datatype in the ISO 21090 datatype classification hierarchy</skos:definition>
		<cmns-col:isMemberOf rdf:resource="&idmp-dtp;ISO21090-CodeSet"/>
		<cmns-txt:hasTextValue>HXIT</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
