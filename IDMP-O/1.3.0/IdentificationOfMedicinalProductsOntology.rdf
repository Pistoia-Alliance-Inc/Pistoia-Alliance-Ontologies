				<owl:oneOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&idmp-sub;SubstanceNameClassifier-BrandName">
					</rdf:Description>
					<rdf:Description rdf:about="&idmp-sub;SubstanceNameClassifier-CommonName">
