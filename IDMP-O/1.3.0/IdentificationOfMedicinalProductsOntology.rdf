	<owl:NamedIndividual rdf:about="&idmp-dtp;DatatypeCode-REAL">
		<rdf:type rdf:resource="&cmns-cds;CodeElement"/>
		<rdfs:label>datatype code REAL</rdfs:label>
		<skos:definition>code for a real number datatype</skos:definition>
		<cmns-av:explanatoryNote>This code denotes a real number typically expressed as an xsd:decimal, xsd:float, owl:rational, or owl:real datatype.</cmns-av:explanatoryNote>
		<cmns-col:isMemberOf rdf:resource="&idmp-dtp;ISO21090-CodeSet"/>
		<cmns-dsg:hasDescription>real number</cmns-dsg:hasDescription>
