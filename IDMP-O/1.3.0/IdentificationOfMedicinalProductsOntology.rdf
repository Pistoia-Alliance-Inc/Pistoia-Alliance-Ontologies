		<skos:definition>code for a name</skos:definition>
		<cmns-av:explanatoryNote>This code denotes a name expressed as an xsd:string or rdf:langString, or a reified name that is an individual of type cmns-dsg:Name.</cmns-av:explanatoryNote>
		<cmns-col:isMemberOf rdf:resource="&idmp-dtp;ISO21090-CodeSet"/>
		<cmns-dsg:hasDescription>name</cmns-dsg:hasDescription>
		<cmns-txt:hasTextValue>NM</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-dtp;DatatypeCode-PQ">
		<rdf:type rdf:resource="&cmns-cds;CodeElement"/>
