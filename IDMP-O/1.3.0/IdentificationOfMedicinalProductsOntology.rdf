				<owl:allValuesFrom rdf:resource="&idmp-sub;StructuralRepeatUnit"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&idmp-sub;hasAmount"/>
				<owl:onClass rdf:resource="&idmp-sub;Amount"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
