		<cmns-av:synonym>depth</cmns-av:synonym>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-mprd;DiameterCharacteristic">
		<rdfs:subClassOf rdf:resource="&idmp-sub;PhysicalCharacteristic"/>
		<rdfs:subClassOf>
			<owl:Restriction>
